{
  "name": "serverless-clean-architecture",
  "version": "1.0.0",
  "main": "serverless.ts",
  "license": "MIT",
  "scripts": {
    "dev": "serverless offline start --httpPort=3333 --reloadHandler --verbose",
    "dev:email": "email dev --dir src/infrastructure/aws/ses/templates",
    "drizzle:studio": "drizzle-kit studio",
    "migrations:generate": "drizzle-kit generate:pg",
    "migrations:push": "ts-node -T src/infrastructure/database/drizzle/scripts/run-migrations.ts",
    "migrations:drop": "drizzle-kit drop",
    "prepare": "husky",
    "test": "vitest run",
    "test:unit": "vitest run --config ./vitest.config.unit.mts",
    "test:unit:watch": "vitest --config ./vitest.config.unit.mts",
    "test:e2e": "vitest run --config ./vitest.config.e2e.mts",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.mts",
    "test:ci": "vitest run --coverage"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.18.0",
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/client-ses": "^3.504.0",
    "@aws-sdk/client-transcribe": "^3.515.0",
    "@aws-sdk/s3-request-presigner": "^3.515.0",
    "@middy/core": "^3.6.2",
    "@middy/do-not-wait-for-empty-event-loop": "^5.2.3",
    "@middy/http-cors": "^5.2.3",
    "@middy/http-json-body-parser": "^3.6.2",
    "@middy/http-security-headers": "^5.2.3",
    "@middy/sqs-partial-batch-failure": "^5.2.3",
    "@react-email/components": "0.0.14",
    "@react-email/render": "0.0.12",
    "axios": "^1.6.7",
    "drizzle-orm": "^0.29.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-email": "2.0.0",
    "serialize-error": "^11.0.3",
    "serverless-offline-ses-v2": "^1.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@faker-js/faker": "^8.4.0",
    "@rocketseat/eslint-config": "^2.1.0",
    "@serverless/typescript": "^3.38.0",
    "@testcontainers/postgresql": "^10.6.0",
    "@types/aws-lambda": "^8.10.132",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.8",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.51",
    "@vitest/coverage-v8": "^1.2.2",
    "dotenv": "^16.4.1",
    "drizzle-kit": "^0.20.13",
    "esbuild": "^0.14.54",
    "eslint": "^8.56.0",
    "husky": "^9.0.6",
    "json-schema-to-ts": "^1.6.5",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.50.2",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^13.3.3",
    "serverless-prune-plugin": "^2.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.15.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  }
}
